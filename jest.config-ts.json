{
  "clearMocks": true,
  "collectCoverageFrom": ["<rootDir>/src/**/*.ts"],
  "coveragePathIgnorePatterns": ["__tests__"],
  "testMatch": ["<rootDir>/src/__tests__/**/**.test.ts"],
  "setupFilesAfterEnv": ["../../jest.setup.js"],
  "transformIgnorePatterns": ["/node_modules/"],
  "testEnvironment": "node",
  "testTimeout": 30000,
  "preset": "ts-jest"
}
