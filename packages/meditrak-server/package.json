  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "main": "handler.js",
  "scripts": {
    "build": "rm -rf dist && babel src -s --out-dir dist --source-maps --ignore '**src/database/migrations','src/tests/**' --copy-files --no-copy-ignored --config-file '../../babel.config.json'",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "lint:fix": "yarn lint --fix .",
    "prestart": "npm run -s build",
    "start": "node dist",
    "start-dev": "../../scripts/bash/backendStartDev.sh 9999",
    "start-verbose": "LOG_LEVEL=debug npm run start-dev",
    "test": "mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha"
