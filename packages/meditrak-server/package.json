{
  "name": "@tupaia/meditrak-server",
  "version": "1.0.0",
  "private": true,
  "description": "REST API for Meditrak",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia.git",
    "directory": "packages/meditrak-server"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "main": "handler.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && run-p -c \"build:* {@}\" --",
    "build:src": "babel src -s --out-dir dist --source-maps --ignore \"**src/database/migrations\",\"src/tests/**\" --copy-files --no-copy-ignored --config-file \"../../babel.config.json\"",
    "build:types": "tsc",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "lint:fix": "yarn lint --fix",
    "prestart": "npm run -s build",
    "start": "node dist",
    "start-dev": "../../scripts/bash/backendStartDev.sh 9999",
    "start-verbose": "LOG_LEVEL=debug npm run start-dev",
    "test": "yarn workspace @tupaia/database check-test-database-exists && DB_NAME=tupaia_test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@tupaia/access-policy": "3.0.0",
    "@tupaia/auth": "1.0.0",
    "@tupaia/data-broker": "1.0.0",
    "@tupaia/database": "1.0.0",
    "@tupaia/dhis-api": "1.0.0",
    "@tupaia/expression-parser": "1.0.0",
    "@tupaia/utils": "1.0.0",
    "adm-zip": "^0.5.5",
    "api-error-handler": "^1.0.0",
    "async": "^2.6.4",
    "aws-sdk": "^2.451.0",
    "body-parser": "^1.18.3",
    "case": "^1.6.1",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "cors": "^2.8.5",
    "countrynames": "^0.1.1",
    "del": "^2.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "form-data": "^2.3.3",
    "format-link-header": "^2.1.0",
    "http": "^0.0.0",
    "lodash.chunk": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "lodash.keyby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "morgan": "^1.9.0",
    "multer": "^1.4.3",
    "nodemailer": "^4.7.0",
    "public-ip": "^2.5.0",
    "react-autobind": "^1.0.6",
    "react-native-uuid": "^1.4.9",
    "s3urls": "^1.5.2",
    "semver-compare": "^1.0.0",
    "string-similarity": "^1.2.2",
    "urlencode": "^1.1.0",
    "winston": "^3.3.3",
    "xlsx": "^0.10.9"
  },
  "devDependencies": {
    "deep-equal-in-any-order": "^1.0.21",
    "sinon-test": "^3.0.0"
  }
}
