{
  "name": "@tupaia/central-server",
  "version": "1.0.0",
  "private": true,
  "description": "Central server for handling all miscellaneous Tupaia/Meditrak behaviour",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia.git",
    "directory": "packages/central-server"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "main": "handler.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && run-p -c \"build:* {@}\" --",
    "build:src": "yarn package:build:js",
    "build:types": "npm run --prefix ../../ package:build:types",
    "build-dev": "npm run build",
    "lint": "yarn package:lint:js",
    "lint:fix": "yarn lint --fix",
    "start": "node dist",
    "start-dev": "yarn package:start:backend-start-dev 9999",
    "start-verbose": "LOG_LEVEL=debug yarn start-dev",
    "test": "yarn workspace @tupaia/database check-test-database-exists && DB_NAME=tupaia_test NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "create-meditrak-sync-view": "node dist/createMeditrakSyncView.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@tupaia/access-policy": "3.0.0",
    "@tupaia/auth": "1.0.0",
    "@tupaia/data-broker": "1.0.0",
    "@tupaia/database": "1.0.0",
    "@tupaia/dhis-api": "1.0.0",
    "@tupaia/expression-parser": "1.0.0",
    "@tupaia/utils": "1.0.0",
    "adm-zip": "^0.5.5",
    "api-error-handler": "^1.0.0",
    "async": "^2.6.2",
    "aws-sdk": "^2.451.0",
    "body-parser": "^1.18.3",
    "case": "^1.6.1",
    "cors": "^2.8.5",
    "countrynames": "^0.1.1",
    "del": "^2.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "form-data": "^2.3.3",
    "format-link-header": "^2.1.0",
    "http": "^0.0.0",
    "lodash.chunk": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "lodash.keyby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.9.0",
    "multer": "^1.4.3",
    "nodemailer": "^4.7.0",
    "public-ip": "^2.5.0",
    "react-autobind": "^1.0.6",
    "react-native-uuid": "^1.4.9",
    "s3urls": "^1.5.2",
    "semver-compare": "^1.0.0",
    "string-similarity": "^1.2.2",
    "urlencode": "^1.1.0",
    "winston": "^3.3.3",
    "xlsx": "^0.10.9"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "cross-env": "^7.0.2",
    "deep-equal-in-any-order": "^1.0.21",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "sinon-test": "^3.0.0",
    "supertest": "^3.1.0"
  }
}
