{
  "name": "@tupaia/admin-panel",
  "version": "1.0.0",
  "private": true,
  "description": "An interface for managing Tupaia data for non-developers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia.git",
    "directory": "packages/admin-panel"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "main": "dist/library.js",
  "scripts": {
    "build": "run-p -c \"build:* {@}\" --",
    "build:src": "npm run --prefix ../../ package:build:react-scripts",
    "build:library": "rm -rf dist && npm run --prefix ../../ package:build:js",
    "build-dev": "npm run build:library",
    "cypress:open": "cross-env NODE_ENV=test yarn cypress open",
    "cypress:run": "cross-env NODE_ENV=test yarn cypress run",
    "eject": "yarn package:eject",
    "lint": "yarn package:lint:js",
    "lint:fix": "yarn lint --fix",
    "start": "yarn package:start:react-scripts",
    "start-dev": "yarn start",
    "start-fullstack": "npm-run-all -c -l -p start-central-server start-entity-server start-report-server start-admin-panel-server start-dev",
    "start-central-server": "yarn workspace @tupaia/central-server start-dev",
    "start-entity-server": "yarn workspace @tupaia/entity-server start-dev",
    "start-report-server": "yarn workspace @tupaia/report-server start-dev",
    "start-admin-panel-server": "yarn workspace @tupaia/admin-panel-server start-dev",
    "test": "echo No tests"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@material-ui/styles": "^4.11.5",
    "@tupaia/access-policy": "3.0.0",
    "@tupaia/types": "1.0.0",
    "@tupaia/ui-components": "1.0",
    "@tupaia/utils": "1.0.0",
    "axios": "^0.21.4",
    "case": "^1.6.3",
    "content-disposition-header": "^0.6.0",
    "date-fns": "^2.29.3",
    "file-saver": "^1.3.3",
    "localforage": "^1.10.0",
    "lodash.debounce": "^4.0.8",
    "lodash.keyby": "^4.6.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.4",
    "parse-link-header": "^2.0.0",
    "prop-types": "^15.8.1",
    "query-string": "^5.1.1",
    "react": "17.0.0",
    "react-autobind": "^1.0.6",
    "react-beforeunload": "^2.5.1",
    "react-datepicker": "^1.8.0",
    "react-dom": "17.0.0",
    "react-hook-form": "^6.15.8",
    "react-icons": "^2.2.7",
    "react-password-strength-bar": "^0.3.5",
    "react-query": "^3.39.3",
    "react-redux": "^5.1.2",
    "react-router-dom": "^5.3.4",
    "react-select": "^1.3.0",
    "react-table": "^6.11.5",
    "reactstrap": "^5.0.0",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.1.0",
    "redux-persist": "6.0.0",
    "redux-thunk": "^2.4.2",
    "styled-components": "^5.3.6",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.0",
    "cross-env": "^7.0.3",
    "cypress-file-upload": "^5.0.8",
    "npm-run-all": "^4.1.5"
  }
}
