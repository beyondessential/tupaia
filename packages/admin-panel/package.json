{
  "name": "@tupaia/admin-panel",
  "version": "1.0.0",
  "private": true,
  "description": "An interface for managing Tupaia data for non-developers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia.git",
    "directory": "packages/admin-panel"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://bes.au)",
  "main": "./lib/library.js",
  "scripts": {
    "build": "run-s -c \"build:* {@}\" --",
    "build:src": "npm run --prefix ../../ package:build:vite",
    "preview": "vite preview",
    "build:library": "rimraf lib && npm run --prefix ../../ package:build:lib",
    "build-dev": "yarn run build:library",
    "lint": "yarn package:lint",
    "lint:fix": "yarn lint --fix",
    "start": "yarn package:start:vite --port 3000",
    "start-dev": "yarn start",
    "start-stack": "yarn package:start-stack admin-panel",
    "test": "echo No tests"
  },
  "browserslist": [
    "defaults"
  ],
