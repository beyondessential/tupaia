{
  "name": "@tupaia/reports-lambda",
  "version": "0.0.0",
  "description": "Lambda Function for building report data",
  "main": "dist",
  "scripts": {
    "lint": "eslint --ext .js  --ignore-pattern __tests__/ .",
    "lint:all": "yarn run lint \"src/**/*.{js,jsx}\"",
    "build": "babel src --out-dir dist --source-maps --config-file '../../babel.config.json'",
    "start": "node dist",
    "start-dev": "../../scripts/bash/backendStartDev.sh 9997",
    "start-verbose": "LOG_LEVEL=debug npm run start-dev",
    "prestart": "npm run -s build",
    "test": "mocha",
    "test-coverage": "cross-env NODE_ENV=test nyc mocha",
    "validate": "babel-node scripts/validate/index.js --config-file '../../babel.config.json'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@tupaia/aggregator": "1.0.0",
    "@tupaia/database": "1.0.0",
    "@tupaia/utils": "1.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {},
  "bugs": {
    "url": "https://github.com/beyondessential/tupaia/issues"
  },
  "homepage": "https://github.com/beyondessential/tupaia"
}
