{
  "name": "@tupaia/web-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Web app that integrates with and displays data from the Tupaia project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia",
    "directory": "packages/web-frontend"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "main": "dist/index.js",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "rm -rf dist && run-s \"build:* {@}\" --",
    "build:desktop": "SKIP_PREFLIGHT_CHECK=true BUILD_PATH='./build/desktop/' GENERATE_SOURCEMAP=false react-scripts build",
    "build:mobile": "SKIP_PREFLIGHT_CHECK=true BUILD_PATH='./build/mobile/' GENERATE_SOURCEMAP=false REACT_APP_APP_TYPE=mobile react-scripts build",
    "cypress:config": "yarn run:babel cypress/scripts/generateConfig",
    "cypress:format-config": "yarn run:babel cypress/scripts/formatConfig",
    "cypress:open": "cross-env NODE_ENV=test yarn cypress open",
    "cypress:run": "cross-env NODE_ENV=test yarn cypress run",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "lint:fix": "yarn lint --fix",
    "run:babel": "babel-node --config-file \"../../babel.config.json\"",
    "start": "PORT=8088 react-scripts start",
    "start-dev": "npm run start",
    "start-fullstack": "npm-run-all -c -l -p start-server start",
    "start-mobile": "REACT_APP_APP_TYPE=mobile PORT=8089 PORT=8088 react-scripts start",
    "start-server": "yarn workspace @tupaia/web-config-server start",
    "storybook": "start-storybook -s public -p 6007",
    "test": "node scripts/test.js --env=jsdom",
    "test-e2e": "yarn run:babel cypress/scripts/testE2e"
  },
  "resolutions": {
    "jss": "10.0.0"
  },
  "dependencies": {
    "@mapbox/polyline": "^0.2.0",
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.9.10",
    "@tupaia/ui-components": "1.0",
    "@tupaia/utils": "1.0.0",
    "core-js": "^3.16.1",
    "dom-to-image": "2.6.0",
    "dotenv": "^8.2.0",
    "downloadjs": "1.4.7",
    "email-validator": "^1.1.1",
    "history": "^4.7.2",
    "is-mobile": "^0.2.2",
    "lodash": "^4.17.4",
    "markdown-to-jsx": "^6.4.1",
    "material-ui": "^0.18.3",
    "material-ui-datetimepicker": "^1.0.7",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "numeral": "^2.0.6",
    "polished": "^3.0.0",
    "prop-types": "^15.6.2",
    "query-string": "^5.0.1",
    "radium": "^0.19.5",
    "re-reselect": "^3.4.0",
    "react": "^16.13.1",
    "react-animations": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-redux": "^5.0.5",
    "react-tap-event-plugin": "^3.0.3",
    "recharts": "^1.8.5",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^1.0.0-beta.2",
    "regenerator-runtime": "^0.13.9",
    "reselect": "^4.0.0",
    "sanitize-filename": "^1.6.3",
    "shallowequal": "^1.0.2",
    "source-map-explorer": "^1.5.0",
    "styled-components": "^5.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@cypress/snapshot": "^2.1.7",
    "@tupaia/database": "1.0.0",
    "case": "^1.6.3",
    "chalk": "1.1.3",
    "cheerio": "^1.0.0-rc.3",
    "fs-extra": "3.0.1",
    "jest": "^27.0.6",
    "js-beautify": "^1.13.0",
    "object-assign": "4.1.1",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7"
  },
  "browserslist": [
    "defaults"
  ]
}
