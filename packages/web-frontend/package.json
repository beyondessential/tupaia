{
  "name": "@tupaia/web-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Web app that integrates with and displays data from the Tupaia project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia",
    "directory": "packages/web-frontend"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "main": "dist/index.js",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "mkdir -p builds && npm run build-desktop && npm run build-mobile && npm run build-exporter",
    "build-desktop": "node scripts/build.js && rm -rf builds/desktop && mv build builds/desktop",
    "build-exporter": "REACT_APP_APP_TYPE=exporter node scripts/build.js && rm -rf builds/exporter && mv build builds/exporter",
    "build-mobile": "REACT_APP_APP_TYPE=mobile node scripts/build.js && rm -rf builds/mobile && mv build builds/mobile",
    "cypress:generate-config": "babel-node cypress/scripts/generateConfig --config-file '../../babel.config.json'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "lint:fix": "yarn lint --fix .",
    "start": "PORT=8088 node scripts/start.js",
    "start-dev": "npm run start",
    "start-exporter": "PORT=3001 REACT_APP_APP_TYPE=exporter node scripts/start.js",
    "start-fullstack": "npm-run-all -c -l -p start-server start",
    "start-mobile": "REACT_APP_APP_TYPE=mobile PORT=8089 node scripts/start.js",
    "start-server": "yarn workspace @tupaia/web-config-server start",
    "storybook": "start-storybook -s ./public -p 6006",
    "test": "node scripts/test.js --env=jsdom",
    "test:cypress:open": "start-server-and-test start-fullstack 8088 cypress:open",
    "test:cypress:run": "start-server-and-test start-fullstack 8088 cypress:run"
  },
  "resolutions": {
    "jss": "10.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@mapbox/polyline": "^0.2.0",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/styles": "^4.7.1",
    "dotenv": "^8.2.0",
    "email-validator": "^1.1.1",
    "history": "^4.7.2",
    "is-mobile": "^0.2.2",
    "leaflet": "^1.1.0",
    "lodash": "^4.17.4",
    "markdown-to-jsx": "^6.4.1",
    "material-ui": "^0.18.3",
    "material-ui-datetimepicker": "^1.0.7",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "numeral": "^2.0.6",
    "polished": "^3.0.0",
    "prop-types": "^15.6.2",
    "query-string": "^5.0.1",
    "radium": "^0.19.5",
    "re-reselect": "^3.4.0",
    "react": "^16.13.1",
    "react-animations": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-leaflet": "^1.4.0",
    "react-redux": "^5.0.5",
    "react-svg-loader": "^2.1.0",
    "react-tap-event-plugin": "^3.0.3",
    "recharts": "^1.8.5",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^1.0.0-beta.2",
    "reselect": "^4.0.0",
    "shallowequal": "^1.0.2",
    "source-map-explorer": "^1.5.0",
    "styled-components": "^4.1.3",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@cypress/snapshot": "^2.1.7",
    "@tupaia/database": "1.0.0",
    "@tupaia/utils": "1.0.0",
    "autoprefixer": "7.1.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "0.28.1",
    "eslint-loader": "^4.0.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0"
  }
}
