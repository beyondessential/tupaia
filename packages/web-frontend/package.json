{
  "name": "@tupaia/web-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Web app that integrates with and displays data from the Tupaia project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia",
    "directory": "packages/web-frontend"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "scripts": {
    "build": "rm -rf dist && run-s \"build:* {@}\" --",
    "build:desktop": "BUILD_PATH='./build/desktop/' npm run --prefix ../../ package:build:react-scripts",
    "build:mobile": "BUILD_PATH='./build/mobile/' REACT_APP_APP_TYPE=mobile npm run --prefix ../../ package:build:react-scripts",
    "lint": "yarn package:lint:js",
    "lint:fix": "yarn lint --fix",
    "start": "PORT=8088 yarn package:start:react-scripts",
    "start-dev": "yarn start",
    "start-mobile": "REACT_APP_APP_TYPE=mobile PORT=8089 PORT=8088 package:start:react-scripts",
    "start-fullstack": "npm-run-all -c -l -p start-central-server start-entity-server start-report-server start-web-config-server start-dev",
    "start-central-server": "yarn workspace @tupaia/central-server start-dev",
    "start-entity-server": "yarn workspace @tupaia/entity-server start-dev",
    "start-report-server": "yarn workspace @tupaia/report-server start-dev",
    "start-web-config-server": "yarn workspace @tupaia/web-config-server start-dev",
    "storybook": "start-storybook -s public -p 6007",
    "test": "node scripts/test.js --env=jsdom"
  },
  "resolutions": {
    "jss": "10.0.0"
  },
  "dependencies": {
    "@mapbox/polyline": "^0.2.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@material-ui/styles": "^4.11.5",
    "@tupaia/ui-components": "1.0",
    "@tupaia/utils": "1.0.0",
    "core-js": "^3.16.1",
    "dom-to-image": "2.6.0",
    "dotenv": "^8.2.0",
    "downloadjs": "1.4.7",
    "email-validator": "^1.1.1",
    "history": "^4.7.2",
    "is-mobile": "^0.2.2",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^6.4.1",
    "material-ui": "^0.18.3",
    "material-ui-datetimepicker": "^1.0.7",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.27",
    "numeral": "^2.0.6",
    "polished": "^3.0.0",
    "prop-types": "^15.8.1",
    "query-string": "^5.1.1",
    "radium": "^0.19.5",
    "re-reselect": "^3.4.0",
    "react": "17.0.0",
    "react-animations": "^1.0.0",
    "react-dom": "17.0.0",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.8.2",
    "react-tap-event-plugin": "^3.0.3",
    "recharts": "^1.8.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.2.2",
    "regenerator-runtime": "^0.13.9",
    "reselect": "^4.0.0",
    "sanitize-filename": "^1.6.3",
    "shallowequal": "^1.0.2",
    "styled-components": "^5.3.6",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@sambego/storybook-state": "^2.0.1",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.0.11",
    "case": "^1.6.3",
    "chalk": "1.1.3",
    "cheerio": "^1.0.0-rc.3",
    "fs-extra": "3.0.1",
    "jest": "^27.0.6",
    "js-beautify": "^1.13.0",
    "mockdate": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "object-assign": "4.1.1",
    "promise": "7.1.1",
    "react-dev-utils": "^3.1.3",
    "react-error-overlay": "^1.0.10",
    "sinon": "^9.0.2"
  },
  "browserslist": [
    "defaults"
  ]
}
