      ]
    },
    {
      "description": "Applies correct aggregation (object format)",
      "input": {
        "indicatorCodes": ["MonthlyBirthIncrease"],
        "fetchOptions": {
          "startDate": "2020-12-01",
          "endDate": "2020-12-31",
          "organisationUnitCodes": ["GR"]
