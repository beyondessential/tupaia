{
  "name": "@tupaia/psss",
  "version": "0.1.0",
  "private": true,
  "description": "Pacific Syndromic Surveillance System client application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia",
    "directory": "packages/psss"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "main": "dist/index.js",
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "cypress:open": "cross-env NODE_ENV=test cypress open",
    "cypress:run": "cross-env NODE_ENV=test cypress run",
    "cypress:config": "NODE_ENV=development  babel-node cypress/scripts/generateConfig --config-file \"../../babel.config.json\"",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "lint:fix": "yarn lint --fix",
    "pretest:test:cypress:run": "npm run build",
    "start-dev": "react-scripts start",
    "start-fullstack": "npm-run-all -c -l -p start-psss-server start-entity-server start-meditrak-server start-report-server  start-dev",
    "start-psss-server": "yarn workspace @tupaia/psss-server start-dev -s",
    "start-entity-server": "yarn workspace @tupaia/entity-server start-dev -s",
    "start-meditrak-server": "yarn workspace @tupaia/meditrak-server start-dev -s",
    "start-report-server": "yarn workspace @tupaia/report-server start-dev -s",
    "test": "npm run test:jest",
    "test:cypress:open": "start-server-and-test start-fullstack 3000 cypress:open",
    "test:cypress:run": "start-server-and-test start-fullstack 3000 cypress:run",
    "test:jest": "react-scripts test --env=jsdom"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.10",
    "@tupaia/ui-components": "1.0",
    "@tupaia/utils": "1.0.0",
    "axios": "^0.21.0",
    "date-fns": "^2.16.1",
    "history": "^5.0.0",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.keyby": "^4.6.0",
    "lodash.orderby": "^4.6.0",
    "msw": "^0.26.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.0.0",
    "react-password-strength-bar": "^0.3.3",
    "react-query": "^2.25.2",
    "react-query-devtools": "^2.6.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@tupaia/auth": "1.0.0",
    "@tupaia/database": "1.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "faker": "^4.1.0",
    "mockdate": "^3.0.5"
  }
}
