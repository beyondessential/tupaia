{
  "name": "@tupaia/expression-parser",
  "version": "1.0.0",
  "private": true,
  "description": "Wraps around mathjs package, used for evaluating expressions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia.git",
    "directory": "packages/expression-parser"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://beyondessential.com.au)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && run-p \"build:* {@}\" --",
    "build:src": "babel src --out-dir dist --source-maps --ignore \"src/__tests__/**\" --config-file \"../../babel.config.json\"",
    "build:types": "tsc",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "mathjs": "^9.4.0"
  }
}
