{
  "name": "@tupaia/utils",
  "version": "1.0.0",
  "private": true,
  "description": "Utility functions that are helpful across multiple packages",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondessential/tupaia.git",
    "directory": "packages/utils"
  },
  "author": "Beyond Essential Systems <admin@tupaia.org> (https://bes.au)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run clean:build && run-p \"build:* {@}\" --",
    "build:src": "yarn run package:build:js",
    "build:types": "yarn run package:build:types",
    "build-dev": "yarn run build",
    "build-watch": "yarn run clean:build && yarn run build:types && yarn run package:build:js-watch",
    "clean:build": "rimraf -- dist",
    "lint": "yarn package:lint",
    "lint:fix": "yarn lint --fix",
    "test": "yarn package:test",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "bson-objectid": "^1.2.2",
    "case": "^1.6.3",
    "countrynames": "^0.1.1",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^2.0.1",
    "fast-memoize": "^2.5.2",
    "jsonwebtoken": "^9.0.0",
    "lodash.get": "^4.4.2",
    "lodash.isplainobject": "^4.0.6",
    "lodash.pickby": "^4.6.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.45",
    "node-fetch": "^1.7.3",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "rrule": "^2.8.1",
    "sanitize-filename": "^1.6.3",
    "validator": "^13.11.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "mockdate": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "sinon": "^9.0.2",
    "yargs": "15.4.1"
  }
}
